#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/charity/Documents/dogData/node_modules/.pnpm/nodemon@3.0.3/node_modules/nodemon/bin/node_modules:/mnt/c/Users/charity/Documents/dogData/node_modules/.pnpm/nodemon@3.0.3/node_modules/nodemon/node_modules:/mnt/c/Users/charity/Documents/dogData/node_modules/.pnpm/nodemon@3.0.3/node_modules:/mnt/c/Users/charity/Documents/dogData/node_modules/.pnpm/node_modules:/mnt/c/Users/charity/Documents/dogData/node_modules:/mnt/c/Users/charity/Documents/node_modules:/mnt/c/Users/charity/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/charity/documents/dogData/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/charity/Documents/dogData/node_modules/.pnpm/nodemon@3.0.3/node_modules/nodemon/bin/node_modules:/mnt/c/Users/charity/Documents/dogData/node_modules/.pnpm/nodemon@3.0.3/node_modules/nodemon/node_modules:/mnt/c/Users/charity/Documents/dogData/node_modules/.pnpm/nodemon@3.0.3/node_modules:/mnt/c/Users/charity/Documents/dogData/node_modules/.pnpm/node_modules:/mnt/c/Users/charity/Documents/dogData/node_modules:/mnt/c/Users/charity/Documents/node_modules:/mnt/c/Users/charity/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/charity/documents/dogData/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/nodemon.js" "$@"
else
  exec node  "$basedir/../../bin/nodemon.js" "$@"
fi
